public class TestRecordBuilder {

    private int x;

    private String y;

    public TestRecordBuilder withX(int x) {
        this.x = x;
        return this;
    }

    public TestRecordBuilder withY(String y) {
        this.y = y;
        return this;
    }

    public TestRecord build() {
        return new TestRecord(x, y);
    }
}
